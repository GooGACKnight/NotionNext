    // Waline 客户端库
    "@waline/client": "^2.5.1",
    // CSS 自动前缀
    "autoprefixer": "^10.4.13",
    // 环境变量设置工具
    "cross-env": "^7.0.3",
    // ESLint 代码检查工具
    "eslint": "^8.57.1",
    // Next.js ESLint 配置
    "eslint-config-next": "^13.1.1",
    // Prettier ESLint 配置
