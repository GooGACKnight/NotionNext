    "next-sitemap": "^1.6.203",
    // CSS 处理器
    "postcss": "^8.4.31",
    // 代码格式化工具
    "prettier": "^3.3.2",
    // Tailwind CSS 框架
    "tailwindcss": "^3.3.2",
    // TypeScript 语言
    "typescript": "5.6.2",
    // Webpack 打包分析器
    "webpack-bundle-analyzer": "^4.5.0"
  },
