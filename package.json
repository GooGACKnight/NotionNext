    "notion-client": "6.15.6",
    // Notion 工具库
    "notion-utils": "6.15.6",
    // React 框架
    "react": "^18.2.0",
    // React DOM 库
    "react-dom": "^18.2.0",
    // React Facebook 组件库
    "react-facebook": "^8.1.4",
