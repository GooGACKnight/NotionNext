{
  // 项目名称
  "name": "notion-next",
  // 版本号
  "version": "4.8.3",
  // 项目主页
  "homepage": "https://github.com/tangly1024/NotionNext.git",
  // 项目许可证
  "license": "MIT",
  // 代码仓库信息
  "repository": {
    "type": "git",
    "url": "https://github.com/tangly1024/NotionNext.git"
